{
  "bindings": [
    {
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "authLevel": "anonymous",
      "methods": [
        "get"
      ],
      "route": "{id:alpha}"
    },
    {
      "name": "links",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "tomi-sh-db",
      "collectionName": "links",
      "connectionStringSetting": "CONFIG_COSMOS_DB_ACCT_CONN",
      "id": "{id}",
      "partitionKey": "{id}"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    }
  ]
}
